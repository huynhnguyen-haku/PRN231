@{
    ViewData["Title"] = "Edit";
}

@model gRPC_Client.Protos.CategoryReply

<div class="form-group row">
    <div class="col-md-6">
        <label for="1">Category ID: </label>
        @Html.TextBoxFor(model => model.CategoryId, new { disabled = "disabled" })
    </div>
</div>
<div class="form-group row">
    <div class="col-md-6">
        <label for="1">Category Name:</label>
        @Html.EditorFor(model => model.CategoryName, new { htmlAttributes = new { @class = "form-control" } })
    </div>
</div>